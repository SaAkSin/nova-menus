{
    "name": "saaksin/nova-menus",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Saaksin\\NovaMenus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saaksin\\NovaMenus\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
